{
  "name": "jssak",
  "version": "1.0.0",
  "description": "Slight and elegant functional utils for modern JavaScript.",
  "main": "dist/jssak.cjs.js",
  "module": "dist/jssak.esm.js",
  "browser": "dist/jssak.umd.js",
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "release": "yarn docs:build && gh-pages -d docs/.vuepress/dist/",
    "lint": "eslint src test",
    "build": "rimraf dist && rollup -c",
    "build:watch": "rollup -c -w",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "coveralls": "cat coverage/lcov.info | coveralls"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "nyc": {
    "require": [
      "@babel/register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "utils",
    "helpers",
    "server",
    "client",
    "browser",
    "functional"
  ],
  "author": "Edgar Li",
  "repository": {
    "type": "git",
    "url": "git://github.com/EdgarAllanzp/jssak.git"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/register": "^7.0.0",
    "babel-plugin-istanbul": "^5.1.0",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "eslint": "^5.10.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.68.1",
    "rollup-plugin-babel": "^4.1.0",
    "vuepress": "^0.14.8"
  }
}
